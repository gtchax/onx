{
    "manifest_version": 3,
    "name": "Onxlabs",
    "version": "0.0.1",
    "description": "Component style migrator.",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png"
    }
  }
  